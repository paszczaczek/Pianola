<UserControl x:Class="Pianola.ClefsVisualTest"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Pianola"
             mc:Ignorable="d">
    <StackPanel Orientation="Horizontal">
        <Border BorderBrush="BlueViolet" BorderThickness="1" Margin="2">
            <local:TrebleClef IsGuidLinesVisible="True" VerticalAlignment="Top" />
        </Border>
        <Border BorderBrush="BlueViolet" BorderThickness="1" Margin="2">
            <local:BassClef IsGuidLinesVisible="True" VerticalAlignment="Top" />
        </Border>
    </StackPanel>
</UserControl>
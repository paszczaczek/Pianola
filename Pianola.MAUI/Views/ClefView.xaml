<?xml version="1.0" encoding="utf-8"?>

<GraphicsView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:maui="clr-namespace:Pianola.MAUI"
             xmlns:drawables="clr-namespace:Pianola.MAUI.Drawables"
             x:Class="Pianola.MAUI.Views.ClefView"
             x:Name="This"
             StartInteraction="OnStartInteraction"
             VerticalOptions="Start"
             WidthRequest="1" 
             HeightRequest="{x:Static drawables:StaffDrawable.Height}">
    <GraphicsView.Drawable>
        <drawables:ClefDrawable View="{x:Reference This}"/>
    </GraphicsView.Drawable>
</GraphicsView>